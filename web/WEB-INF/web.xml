<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>validaLogin</servlet-name>
        <servlet-class>servlet.validaLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CadastraLogin</servlet-name>
        <servlet-class>servlet.CadastraLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CadastraLugar</servlet-name>
        <servlet-class>servlet.CadastraLugar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Controller</servlet-name>
        <servlet-class>servlet.Controller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetLugar</servlet-name>
        <servlet-class>servlet.GetLugar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeletaLugar</servlet-name>
        <servlet-class>servlet.DeletaLugar</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>validaLogin</servlet-name>
        <url-pattern>/validaLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CadastraLogin</servlet-name>
        <url-pattern>/CadastraLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CadastraLugar</servlet-name>
        <url-pattern>/CadastraLugar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Controller</servlet-name>
        <url-pattern>/Controller</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetLugar</servlet-name>
        <url-pattern>/GetLugar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeletaLugar</servlet-name>
        <url-pattern>/DeletaLugar</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
